<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.elkanah.roemichs.ui.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_student_Dashboard"
            app:destination="@id/student_Dashboard" />
    </fragment>
    <fragment
        android:id="@+id/parentFragment"
        android:name="com.elkanah.roemichs.ui.fragments.ParentFragment"
        android:label="fragment_parent"
        tools:layout="@layout/fragment_parent" />
    <fragment
        android:id="@+id/student_Dashboard"
        android:name="com.elkanah.roemichs.ui.fragments.Student_Dashboard"
        android:label="fragment_student_dashboard"
        tools:layout="@layout/fragment_student_dashboard" />
</navigation>